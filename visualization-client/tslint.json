{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-consistent-codestyle",
        "tslint-immutable"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "no-console": [
            false
        ],
        "ter-indent": [
            true,
            4
        ]
    },
    "rulesDirectory": []
}